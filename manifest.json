{
  "name": "nsfw-detector",
  "version": "1.0.0",
  "description": "NSFW content detection plugin for Forge UI",
  "main": "app.py",
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "fastapi": ">=0.68.0",
    "python-multipart": ">=0.0.5",
    "python-magic": ">=0.4.24",
    "Pillow": ">=8.3.1",
    "python-docx": ">=0.8.11",
    "python-doc": ">=0.1.0",
    "PyPDF2": ">=2.0.0",
    "opencv-python": ">=4.5.3",
    "numpy": ">=1.21.2"
  },
  "forge": {
    "api": "1.0.0",
    "type": "extension",
    "name": "NSFW Detector",
    "description": "Detect NSFW content in images, videos, PDFs, and documents",
    "version": "1.0.0",
    "author": "Your Name",
    "repository": "https://github.com/GeeZeeS/nsfw-detector",
    "entry_point": "app:register",
    "endpoints": [
      {
        "path": "/api/nsfw/check",
        "method": "POST",
        "description": "Check if content is NSFW",
        "auth": true
      },
      {
        "path": "/api/nsfw/health",
        "method": "GET",
        "description": "Health check endpoint",
        "auth": false
      }
    ],
    "settings": {
      "max_file_size": {
        "type": "number",
        "default": 10485760,
        "description": "Maximum file size in bytes"
      }
    }
  }
} 